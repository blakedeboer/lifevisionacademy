<p id="notice"><%= notice %></p>

<p>
  <strong>Project name:</strong>
  <%= @project.project_name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @project.description %>
</p>

<p>
  <strong>Project img:</strong>
  <%= @project.project_img %>
</p>

<p>
  <strong>Project items:</strong>
  <br>
  <% @project.items.each do |item| %>
    <%= link_to item.name, [:admin, item] %><br>
  <% end %>
</p>


<%= link_to 'Edit', edit_admin_project_path(@project) %> |
<%= link_to 'Back', admin_projects_path %>
